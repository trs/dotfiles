#!/usr/bin/env bash

eval "$(starship init ${SHELL##*/})"

export PATH="$HOME/.bin:$PATH"

export VOLTA_HOME="$HOME/.volta"
export PATH="$VOLTA_HOME/bin:$PATH"

export PATH="$SHELLRC_DIR/git:$PATH"

if [ -f "$SHELLRC_DIR/.inputrc" ]; then
  . "$SHELLRC_DIR/.inputrc"
fi

if [ -f "$SHELLRC_DIR/.aliases" ]; then
  . "$SHELLRC_DIR/.aliases"
fi
